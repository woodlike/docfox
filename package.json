{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "description": "docs",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/@types/jest"
    ]
  },
  "scripts": {
    "build:theme": "yarn workspace gatsby-theme-docs build",
